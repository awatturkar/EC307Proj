<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ggkaw.caces.doby">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewClassPage"
            android:label="New Class"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ggkaw.caces.doby.HomePage" />
        </activity>
        <activity
            android:name=".AssignmentPage"
            android:label="Assignment Page"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ggkaw.caces.doby.HomePage" />
        </activity>
        <activity
            android:name=".DeletePage"
            android:label="Delete Page"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ggkaw.caces.doby.HomePage" />
        </activity>
        <activity
            android:name=".AddHomeworkTime"
            android:label="Add Homework"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ggkaw.caces.doby.HomePage" />
        </activity>
    </application>

</manifest>