<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ggkaw.caces.doby">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bird"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/bird"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewClassPage"
            android:label="Add New Class"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ggkaw.caces.doby.HomePage" />
        </activity>
        <activity
            android:name=".AssignmentPage"
            android:label="Add Major Assignment"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ggkaw.caces.doby.HomePage" />
        </activity>
        <activity
            android:name=".DeletePage"
            android:label="Delete Page"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ggkaw.caces.doby.HomePage" />
        </activity>
        <activity
            android:name=".AddHomeworkTime"
            android:label="Add Study Time"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ggkaw.caces.doby.HomePage" />
        </activity>
        <activity
            android:name=".Weather"
            android:label="Weather"
            android:parentActivityName=".HomePage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ggkaw.caces.doby.HomePage" />
        </activity>
        <activity android:name=".Help"></activity>
    </application>

</manifest>